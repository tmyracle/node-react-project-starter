{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User",
  "description": "The User",
  "required": ["email", "first_name", "last_name"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the user."
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The users email. Must be unique.",
      "examples": ["tyler.myracle@gmail.com"]
    },
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "title": "Hash of the most recent pin code"
    },
    "first_name": {
      "$id": "#/properties/first_name",
      "type": "string",
      "title": "The users first name.",
      "examples": ["Tyler"]
    },
    "last_name": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "The users last name.",
      "examples": ["Myracle"]
    },
    "date_of_birth": {
      "$id": "#/properties/date_of_birth",
      "type": "string",
      "title": "The users date of birth.",
      "examples": ["04/23/1991"]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    }
  }
}
