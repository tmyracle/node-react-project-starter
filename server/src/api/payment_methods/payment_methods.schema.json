{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Payment Method",
  "description": "A Payment Method",
  "required": ["type"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the Payment Method."
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The type of the Payment Method",
      "examples": ["card"]
    },
    "profile_id": {
      "$id": "#/properties/profile_id",
      "type": "integer",
      "title": "The database ID of the Profile"
    }
  }
}
