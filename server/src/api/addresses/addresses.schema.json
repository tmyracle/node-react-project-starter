{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Address",
  "description": "Addresses for shippping and payments",
  "required": [
    "address_line_1",
    "country",
    "locality",
    "postal_code",
    "region",
    "type",
    "profile_id",
    "user_id"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the address."
    },
    "address_line_1": {
      "$id": "#/properties/address_line_1",
      "type": "string",
      "title": "First line of an address",
      "examples": ["1553 Mesquite"]
    },
    "address_line_2": {
      "$id": "#/properties/address_line_2",
      "type": "string",
      "title": "Second line of an address",
      "examples": ["Unit N402"]
    },
    "address_line_3": {
      "$id": "#/properties/address_line_3",
      "type": "string",
      "title": "Third line of an address"
    },
    "country": {
      "$id": "#/properties/country",
      "type": "string",
      "title": "Country for the address",
      "examples": ["United States"]
    },
    "is_default": {
      "$id": "#/properties/first_name",
      "type": "boolean",
      "title": "Determine whether this is the default address for the user"
    },
    "locality": {
      "$id": "#/properties/locality",
      "type": "string",
      "title": "Typically something like a city",
      "examples": ["Wichita Falls"]
    },
    "postal_code": {
      "$id": "#/properties/postal_code",
      "type": "string",
      "title": "Postal code for the address.",
      "examples": ["76302-1433"]
    },
    "region": {
      "$id": "#/properties/region",
      "type": "string",
      "title": "State or province for the address",
      "examples": ["TX"]
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "What type of address it is",
      "examples": ["ADDRESS_TYPE_SHIPPING", "ADDRESS_TYPE_BILLING"]
    },
    "profile_id": {
      "$id": "#/properties/profile_id",
      "type": "integer",
      "title": "The id of the profile."
    },
    "user_id": {
      "$id": "#/properties/user_id",
      "type": "integer",
      "title": "The id of the user."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the address."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the address was last updated."
    }
  }
}
