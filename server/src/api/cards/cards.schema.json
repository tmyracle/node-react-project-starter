{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Card",
  "description": "Card used for payment",
  "required": [
    "brand",
    "cardholder_name",
    "exp_month",
    "exp_year",
    "last_4",
    "zipcode"
  ],
  "additionalProperties": false,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The database ID of the card."
    },
    "brand": {
      "$id": "#/properties/brand",
      "type": "string",
      "title": "Brand of the card",
      "examples": ["visa", "amex"]
    },
    "card_cvc": {
      "$id": "#/properties/card_cvc",
      "type": "string",
      "title": "CVC for the card"
    },
    "cardholder_name": {
      "$id": "#/properties/cardholder_name",
      "type": "string",
      "title": "Name of the person who owns the card",
      "examples": ["Tyler Myracle"]
    },
    "exp_month": {
      "$id": "#/properties/exp_month",
      "type": "string",
      "title": "Expiration month of the card",
      "examples": ["05"]
    },
    "exp_year": {
      "$id": "#/properties/exp_year",
      "type": "string",
      "title": "Expiration year of the card",
      "examples": ["22"]
    },
    "is_giftcard": {
      "$id": "#/properties/is_giftcard",
      "type": "boolean",
      "title": "Is the card a giftcard?",
      "examples": ["false"]
    },
    "last_4": {
      "$id": "#/properties/last_4",
      "type": "string",
      "title": "Last 4 digits of the card number",
      "examples": ["3000"]
    },
    "zipcode": {
      "$id": "#/properties/zipcode",
      "type": "string",
      "title": "Zip or postal code associated with the card",
      "examples": ["76302"]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the card."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the card was last updated."
    }
  }
}
